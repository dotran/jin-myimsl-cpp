    libdirs="$with_pkgdir/lib $libdirs"
fi

# ----------------------------------------------------------------------------
# tcl.h
# ----------------------------------------------------------------------------

if test "$enable_tcl" = "yes"; then
    if test -z "$TCLINCDIR"; then


    echo "$as_me:$LINENO: checking for tcl.h" >&5
echo $ECHO_N "checking for tcl.h... $ECHO_C" >&6
    TCLINCDIR=""


    for dir in $incdirs; do
	if test -r "$dir/tcl.h"; then
	    TCLINCDIR="$dir"
	    echo "$as_me:$LINENO: result: $dir/tcl.h" >&5
echo "${ECHO_T}$dir/tcl.h" >&6
	    break
	fi
    done


    if test -z "$TCLINCDIR"; then

	    echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
	    echo "$as_me:$LINENO: result: warning: can't find tcl.h, setting enable_tcl to no" >&5
echo "${ECHO_T}warning: can't find tcl.h, setting enable_tcl to no" >&6
	    enable_tcl="no"

    fi
    if test "$TCLINCDIR" = "/usr/include"; then
	TCLINCDIR="default"
    fi


    fi
fi

